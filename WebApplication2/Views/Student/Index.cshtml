@model List<WebApplication2.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>

        <td>Id</td>
        <td>Fname</td>
        <td>Lname</td>
        <td>City</td>
        <td>Edit</td>

        <td>Delete</td>
        <td>ChechBox</td>

    </tr>
    @foreach (var item in Model)
    {
<tr>
 
    <td>@item.Id</td>
    <td>@item.fname</td>
    <td>@item.lname</td>
    <td>@item.city</td>


    <td>

        @Html.ActionLink("Edit", "Registration", new { id = item.Id })

    </td>
    <td>@Html.ActionLink("Delete", "Delete", new { id = item.Id })</td>
    <td><input type="checkbox" value="" /></td>
</tr>

    }


</table>

<input type="button" value="Delete All" name="button">


@Html.ActionLink("Registration","Registration")